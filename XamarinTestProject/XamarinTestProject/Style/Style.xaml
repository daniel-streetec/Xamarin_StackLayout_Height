<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <OnPlatform x:TypeArguments="x:String" x:Key="BoldFont">
        <On Platform="Android" Value="GothamBold.ttf#Gotham-Bold" />
        <On Platform="iOS" Value="GothamBold" />
    </OnPlatform>
    <OnPlatform x:TypeArguments="x:String" x:Key="MediumFont">
        <On Platform="Android" Value="GothamMedium.ttf#Gotham-Medium" />
        <On Platform="iOS" Value="GothamMedium" />
    </OnPlatform>


</ResourceDictionary>